  <ItemGroup Condition=" '$(TargetFramework)' == '$(TargetFrameworkAndroid9)' ">
    <PackageReference Include="Xamarin.Android.Support.CustomTabs" Version="28.0.0.3" />
  </ItemGroup>
  <ItemGroup Condition=" '$(TargetFramework)' == '$(TargetFrameworkAndroid10)' ">
    <PackageReference Include="Xamarin.AndroidX.Browser" Version="1.0.0" />
  </ItemGroup>

  <!-- This is here to workaround designer issues so VS sees them appropriately -->
  <ItemGroup>
    <None Update="$(PathToMsalSources)\Platforms\netdesktop\CustomWebBrowser.CustomWebBrowserEvent.cs" SubType="Component" />
    <None Update="$(PathToMsalSources)\Platforms\netdesktop\CustomWebBrowser.cs" SubType="Component" />
    <None Update="$(PathToMsalSources)\Platforms\netdesktop\WinFormWebAuthenticationDialog.cs" SubType="Form" />
    <None Update="$(PathToMsalSources)\Platforms\netdesktop\WindowsFormsWebAuthenticationDialogBase.cs" SubType="Form" />
    <None Update="$(PathToMsalSources)\Platforms\netdesktop\SilentWindowsFormsAuthenticationDialog.cs" SubType="Form" />
  </ItemGroup>

  <ItemGroup Label="Build Tools" Condition="$([MSBuild]::IsOsPlatform('Windows'))">
    <PackageReference Include="Microsoft.SourceLink.GitHub" Version="1.0.0" PrivateAssets="All"/>
  </ItemGroup>

</Project>
